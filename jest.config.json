{
  "collectCoverage": true,
  "collectCoverageFrom": ["src/**/*.ts", "!src/**/*.interface.ts"],
  "coverageDirectory": "<rootDir>/coverage",
  "moduleFileExtensions": ["js", "ts"],
  "rootDir": ".",
  "transform": {
    "^.+\\.tsx?$": "ts-jest"
  },
  "testRegex": "test.spec.ts",
  "verbose": true
}
